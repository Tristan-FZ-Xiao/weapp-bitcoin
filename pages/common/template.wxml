<!-- 新闻列表项模板 -->
<template name="newsItem">
  <navigator url="/pages/detail/detail?id={{ id }}" class="article-nav line-bottom" hover-class="tapback">
    <view class="article-item">
      <view class="article-info">
        <view class="article-title line-2 vertical">
          <text>{{ title }}</text>
        </view>
        <view class="article-desc clearfix">
          <view class="article-resource">
            来源：<text>{{ resource }}</text>
          </view>
          <view class="article-date">
            <text>{{ timestamp }}</text>
          </view>
        </view>
      </view>
      <view class="article-thumb">
        <image src="{{ img }}" class="article-image" mode="scaleToFill"></image>
      </view>
    </view>
  </navigator>
</template>

<!-- 新闻条目模板 -->
<template name="articleItem">
  <navigator url="/pages/detail/detail?id={{ id }}" class="news-nav" hover-class="tapback">
    <view class="news-item line-bottom">
      <view class="news-title line-2 vertical">
        <text>{{ title }}</text>
      </view>
      <view class="news-thumb">
        <image src="{{ img }}" class="news-image" mode="scaleToFill"></image>
      </view>
    </view>
  </navigator>
</template>

<template name="footer">
  <navigator url="/pages/detail/detail?id={{ id }}" class="footer-nav" hover-class="tapback">
    <view class="footer-wrap line-bottom">
      <text class="footer-txt">更多比特币科普文章</text>
      <image class="footer-arr" src="/images/icon_right_arrow.png"></image>
    </view>
  </navigator>
</template>

<!-- 无网络模板 -->
<template name="noNetwork">
  <view class="wrapper-empty" bindtap="reloadData">
    <view class="page-empty">
      <view>
        <image class="empty-girl" src="data:image/png;base64," alt=""></image>
      </view>
      <text class="empty-tips">{{errorMsg || "暂时没有数据哦~"}}</text>
    </view>
  </view>
</template>

<!-- 加载中模板 -->
<template name="loadingitem">
  <view class="wrapper-empty">
    <view class="page-empty">
      <view>
        <image class="running-girl" src="data:image/gif;base64," alt=""></image>
      </view>
      <text class="empty-tips">正在加载中...</text>
    </view>
  </view>
</template>